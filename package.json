{
  "name": "mortgage-ui-kit",
  "version": "1.0.0",
  "description": "UI Library",
  "main": "dist/index.js",
  "style": "dist/main.css",
  "scripts": {
    "build": "webpack --mode=production",
    "test": "jest --coverage",
    "test:silent": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:silent"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js}": [
      "pretty-quick --staged",
      "git add"
    ]
  },
  "repository": "https://github.com/MrCrazyBelka/mortgage-ui-kit",
  "author": "KirillTK",
  "license": "MIT",
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "classnames": "^2.2.6",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.9.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "cssnano": "^4.1.10",
    "fork-ts-checker-webpack-plugin": "^4.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "parcel-bundler": "^1.12.4",
    "postcss-loader": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "pretty-quick": "^2.0.1",
    "jest": "^25.1.0",
    "@types/jest": "^25.1.4",
    "ts-jest": "^25.2.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2"
  }
